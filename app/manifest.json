{
    "name": "Quack",
    "description": "Quack chat client",
    "version": "2",
    "key": "",
    "manifest_version": 2,
    "default_locale": "en",
    "oauth2": {
        "client_id": "619186862683-ruvkfri4er379bdc9mn35t2k8ec25bh8.apps.googleusercontent.com",
        "scopes": [
            "https://www.googleapis.com/auth/plus.login"
        ]
    },
    "permissions": [
        {
            "socket": [
                "tcp-listen:*:*",
                "tcp-connect:*:*"
            ]
        },
        "identity",
        "app.window",
        "clipboardRead",
        "clipboardWrite",
        "contextMenus",
        "fullscreen",
        "notifications",
        "pushMessaging",
        "storage",
        "unlimitedStorage",
        "<all_urls>",
        "http://*/*",
        "https://*/*",
        "http://127.0.0.1/*"
    ],
    "icons": {
        "16": "images/icon-16.png",
        "128": "images/icon-128.png"
    },
    "app": {
        "background": {
            "scripts": [
                "scripts/main.js",
                "scripts/chromereload.js"
            ]
        }
    }
}